    <string name="integrity_check_continue_anyway">Napriek tomu pokračovať</string>
    <string name="integrity_check_insufficient_space">Kontrola databázy využíva veľké množstvo dočasného úložiska.\n\nPredtým než budete pokračovať, odporúčame, aby ste mali aspoň %s voľného miesta vo vašom zariadení.</string>
    <string name="integrity_check_insufficient_space_extra_content">\n\nMomentálne máte %s voľného miesta.</string>
    <string name="integrity_check_fixed_no_home_deck">%d vymazaných kartičiek s nesprávnym domovským balíčkom. Prosím, pozrite si manuál pre viac informácií.</string>
